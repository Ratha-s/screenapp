<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobocart"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
     <action android:name="com.mobocart.intent.action.INVOKE_mobocart"></action>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.mobocart.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.mobocart.SecondActivity"
            android:label="@string/title_activity_second" >
        </activity>
        <activity
            android:name="com.example.mobocart.SignupActivity"
            android:label="@string/title_activity_signup" >
        </activity>
        <activity
            android:name="com.example.mobocart.FeatureActivity"
            android:label="@string/title_activity_feature" >
        </activity>
        <activity
            android:name="com.example.mobocart.EnterAmountActivity"
            android:label="@string/title_activity_enter_amount" >
        </activity>
    </application>

</manifest>
